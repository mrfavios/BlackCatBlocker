{
  "manifest_version": 3,
  "name": "BlackCatBlocker",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "48": "icon.png"
  },
  "permissions": ["scripting","cookies","tabs", "activeTab", "storage", "webRequest", "webRequestBlocking", "webNavigation"],
  "host_permissions": [
  "<all_urls>",
  "*://*/*"
],
   "action": {
    "default_popup": "popup.html"
  }

  
}

